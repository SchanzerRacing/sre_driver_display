                    </attributes>
                  </object>
                </child>
                <child>
                  <object class="GtkLabel" id="label_carstate_vehicleinfo">
                    <property name="margin-top">5</property>
                    <property name="label" translatable="1">Carstate</property>
                    <attributes>
                      <attribute name="font-desc" value="Sans 15"></attribute>
                    </attributes>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkBox" id="box_right_vehicleinfo">
                <property name="orientation">vertical</property>
