                        </attributes>
                        <layout>
                          <property name="column">1</property>
                          <property name="row">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="info_temp_motor_fl_vehicleinfo">
                        <property name="label" translatable="1">0°C</property>
                        <attributes>
                          <attribute name="font-desc" value="Sans 30"></attribute>
                        </attributes>
                        <layout>
                          <property name="column">0</property>
                          <property name="row">2</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="info_temp_motor_fr_vehicleinfo">
                        <property name="label" translatable="1">0°C</property>
                        <attributes>
                          <attribute name="font-desc" value="Sans 30"></attribute>
                        </attributes>
                        <layout>
                          <property name="column">1</property>
                          <property name="row">2</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="label_temp_motor_fl_vehicleinfo">
