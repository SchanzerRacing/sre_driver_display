<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkWindow" id="vehicle_info_panel">
    <property name="default-width">800</property>
    <property name="default-height">480</property>
    <property name="child">
      <object class="GtkBox" id="box_main_vehicleinfo">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkFixed" id="fixed_header_vehicleinfo">
            <child>
              <object class="GtkLabel" id="lbl_debug_mv_panel_vehicleinfo">
                <layout>
                  <property name="transform">translate(200, 0)</property>
                </layout>
                <property name="width-request">400</property>
                <property name="height-request">50</property>
                <property name="label" translatable="1">VEHICLE INFO</property>
                <property name="justify">fill</property>
                <attributes>
                  <attribute name="font-desc" value="Sans 20"></attribute>
                </attributes>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label_tsa_vehicleinfo">
                <property name="name">label_tsa</property>
                <property name="width-request">100</property>
                <property name="height-request">50</property>
                <property name="label" translatable="1">TSA</property>
                <attributes>
                  <attribute name="font-desc" value="Sans 20"></attribute>
                </attributes>
              </object>
            </child>
            <child>
              <object class="GtkLabel" id="label_r2d_vehicleinfo">
                <property name="name">label_r2d</property>
                <layout>
                  <property name="transform">translate(700, 0)</property>
                </layout>
                <property name="width-request">100</property>
                <property name="height-request">50</property>
                <property name="label" translatable="1">R2D</property>
                <attributes>
                  <attribute name="font-desc" value="Sans 20"></attribute>
                </attributes>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="box_sidebyside_vehicleinfo">
            <property name="height-request">380</property>
            <child>
              <object class="GtkBox" id="box_left_vehicleinfo">
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkGrid" id="grid_temps_vehicleinfo">
                    <child>
                      <object class="GtkLabel" id="info_temp_pef_vehicleinfo">
                        <property name="width-request">150</property>
                        <property name="margin-top">20</property>
                        <property name="label" translatable="1">0°C</property>
                        <attributes>
                          <attribute name="font-desc" value="Sans 30"></attribute>
                        </attributes>
